{
  "name": "mindlink",
  "version": "1.0.0",
  "type": "module",
  "description": "Local LLM API bridge with Cloudflare tunneling",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "start": "tauri dev",
    "pack": "tauri build --debug",
    "dist": "tauri build",
    "bifrost:build": "./scripts/build-bifrost.sh",
    "bifrost:build:force": "./scripts/build-bifrost.sh --force",
    "bifrost:test": "./scripts/test-bifrost-build.sh",
    "bifrost:test:dry": "./scripts/test-bifrost-build.sh --dry-run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "llm",
    "api",
    "openai",
    "chatgpt",
